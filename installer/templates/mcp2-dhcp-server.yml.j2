mcp:
  user: "{{ mcp_user }}"
  password: "{{ mcp_password }}"
  region: "{{ mcp_region }}"

network:
  interface:  "{{ dhcp_interface }}"
  vlan_id:    "{{ mcp_vlan_id }}"
  service_ip: "{{ private_ipv4 }}"

ipxe:
  enable:       true
  port:         {{ ipxe_port }}
  boot_image:   "{{ ipxe_boot_image }}"
  boot_script:  "{{ ipxe_boot_script }}"
